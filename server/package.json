      "class-validator": "^0.10.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^3.21.1",
      "jsonwebtoken": "^8.5.1",
      "linkpreview-for-node": "^1.1.1",
      "lodash": "^4.17.15",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.25"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
